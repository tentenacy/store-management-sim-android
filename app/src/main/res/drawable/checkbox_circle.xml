<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="16dp"
        android:height="16dp"
        android:left="6dp"
        android:gravity="center_vertical">
        <selector>
            <item android:drawable="@drawable/ic_checked_circle" android:state_checked="true">
