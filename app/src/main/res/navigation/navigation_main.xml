<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_main.xml"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.tenutz.storemngsim.ui.login.LoginFragment"
        android:label="LoginFragment"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_loginFragment_to_signupFormFragment"
            app:destination="@id/signupFormFragment" />
        <action
            android:id="@+id/action_loginFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/signupFormFragment"
        android:name="com.tenutz.storemngsim.ui.signup.SignupFormFragment"
        android:label="SignupFormFragment"
        tools:layout="@layout/fragment_signup_form">
        <action
            android:id="@+id/action_signupFormFragment_to_signupSuccessFragment"
            app:destination="@id/signupSuccessFragment" />
    </fragment>
    <fragment
        android:id="@+id/signupSuccessFragment"
        android:name="com.tenutz.storemngsim.ui.signup.SignupSuccessFragment"
        android:label="SignupSuccessFragment"
        tools:layout="@layout/fragment_signup_success">
        <action
            android:id="@+id/action_signupSuccessFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/mainFragment"
        android:name="com.tenutz.storemngsim.ui.main.MainFragment"
        android:label="MainFragment"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_mainFragment_to_menuMngFragment"
            app:destination="@id/menuMngFragment" />
        <action
            android:id="@+id/action_mainFragment_to_reviewsFragment"
            app:destination="@id/reviewsFragment" />
        <action
            android:id="@+id/action_mainFragment_to_helpsFragment"
            app:destination="@id/helpsFragment" />
        <action
            android:id="@+id/action_mainFragment_to_statisticsFragment"
            app:destination="@id/statisticsFragment" />
    </fragment>
    <fragment
        android:id="@+id/menuMngFragment"
        android:name="com.tenutz.storemngsim.ui.menu.MenuMngFragment"
        android:label="MenuMngFragment"
        tools:layout="@layout/fragment_menu_mng">
        <action
            android:id="@+id/action_menuMngFragment_to_optionMenusFragment"
            app:destination="@id/optionMenusFragment" />
        <action
            android:id="@+id/action_menuMngFragment_to_optionGroupsFragment"
            app:destination="@id/optionGroupsFragment" />
        <action
            android:id="@+id/action_menuMngFragment_to_navigation_main_category"
            app:destination="@id/navigation_main_category" />
        <action
            android:id="@+id/action_menuMngFragment_to_navigation_main_menu"
            app:destination="@id/navigation_main_menu" />
    </fragment>
    <fragment
        android:id="@+id/optionMenusFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optionmenu.OptionMenusFragment"
        android:label="OptionMenusFragment"
        tools:layout="@layout/fragment_option_menus">
        <action
            android:id="@+id/action_optionMenusFragment_to_optionMenusEditFragment"
            app:destination="@id/optionMenusEditFragment" />
        <action
            android:id="@+id/action_optionMenusFragment_to_optionMenuAddFragment"
            app:destination="@id/optionMenuAddFragment" />
        <action
            android:id="@+id/action_optionMenusFragment_to_optionMenuDetailsFragment"
            app:destination="@id/optionMenuDetailsFragment" />
        <action
            android:id="@+id/action_optionMenusFragment_to_omOptionGroupsFragment"
            app:destination="@id/omOptionGroupsFragment" />
    </fragment>
    <fragment
        android:id="@+id/optionMenusEditFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optionmenu.OptionMenusEditFragment"
        android:label="OptionMenusEditFragment"
        tools:layout="@layout/fragment_option_menus_edit" />
    <fragment
        android:id="@+id/optionMenuAddFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optionmenu.OptionMenuAddFragment"
        android:label="OptionMenuAddFragment"
        tools:layout="@layout/fragment_option_menu_add" />
    <fragment
        android:id="@+id/optionMenuDetailsFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optionmenu.OptionMenuDetailsFragment"
        android:label="OptionMenuDetailsFragment"
        tools:layout="@layout/fragment_option_menu_details" />
    <fragment
        android:id="@+id/omOptionGroupsFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optionmenu.optiongroup.OmOptionGroupsFragment"
        android:label="OmOptionGroupsFragment"
        tools:layout="@layout/fragment_om_option_groups">
        <action
            android:id="@+id/action_omOptionGroupsFragment_to_omOptionGroupsEditFragment"
            app:destination="@id/omOptionGroupsEditFragment" />
    </fragment>
    <fragment
        android:id="@+id/omOptionGroupsEditFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optionmenu.optiongroup.OmOptionGroupsEditFragment"
        android:label="OmOptionGroupsEditFragment"
        tools:layout="@layout/fragment_om_option_groups_edit" />
    <fragment
        android:id="@+id/optionGroupsFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optiongroup.OptionGroupsFragment"
        android:label="OptionGroupsFragment"
        tools:layout="@layout/fragment_option_groups">
        <action
            android:id="@+id/action_optionGroupsFragment_to_optionGroupsEditFragment"
            app:destination="@id/optionGroupsEditFragment" />
        <action
            android:id="@+id/action_optionGroupsFragment_to_optionGroupAddFragment"
            app:destination="@id/optionGroupAddFragment" />
        <action
            android:id="@+id/action_optionGroupsFragment_to_optionGroupDetailsFragment"
            app:destination="@id/optionGroupDetailsFragment" />
        <action
            android:id="@+id/action_optionGroupsFragment_to_ogMappingMenusFragment"
            app:destination="@id/ogMappingMenusFragment" />
    </fragment>
    <fragment
        android:id="@+id/optionGroupsEditFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optiongroup.OptionGroupsEditFragment"
        android:label="OptionGroupsEditFragment"
        tools:layout="@layout/fragment_option_groups_edit" />
    <fragment
        android:id="@+id/optionGroupAddFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optiongroup.OptionGroupAddFragment"
        android:label="OptionGroupAddFragment"
        tools:layout="@layout/fragment_option_group_add" />
    <fragment
        android:id="@+id/optionGroupDetailsFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optiongroup.OptionGroupDetailsFragment"
        android:label="OptionGroupDetailsFragment"
        tools:layout="@layout/fragment_option_group_details" />
    <fragment
        android:id="@+id/ogMappingMenusFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optiongroup.OgMappingMenusFragment"
        android:label="OgMappingMenusFragment"
        tools:layout="@layout/fragment_og_mapping_menus">
        <action
            android:id="@+id/action_ogMappingMenusFragment_to_ogOptionMenusEditFragment"
            app:destination="@id/ogOptionMenusEditFragment" />
        <action
            android:id="@+id/action_ogMappingMenusFragment_to_ogOptionMenuAddFragment"
            app:destination="@id/ogOptionMenuAddFragment" />
        <action
            android:id="@+id/action_ogMappingMenusFragment_to_ogMainMenusEditFragment"
            app:destination="@id/ogMainMenusEditFragment" />
        <action
            android:id="@+id/action_ogMappingMenusFragment_to_ogMainMenuAddFragment"
            app:destination="@id/ogMainMenuAddFragment" />
    </fragment>
    <fragment
        android:id="@+id/ogOptionMenusEditFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optiongroup.optionmenu.OgOptionMenusEditFragment"
        android:label="OgOptionMenusEditFragment"
        tools:layout="@layout/fragment_og_option_menus_edit" />
    <fragment
        android:id="@+id/ogOptionMenuAddFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optiongroup.optionmenu.OgOptionMenuAddFragment"
        android:label="OgOptionMenuAddFragment"
        tools:layout="@layout/fragment_og_option_menu_add" />
    <fragment
        android:id="@+id/ogMainMenusEditFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optiongroup.mainmenu.OgMainMenusEditFragment"
        android:label="OgMainMenusEditFragment"
        tools:layout="@layout/fragment_og_main_menus_edit" />
    <fragment
        android:id="@+id/ogMainMenuAddFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optiongroup.mainmenu.OgMainMenuAddFragment"
        android:label="OgMainMenuAddFragment"
        tools:layout="@layout/fragment_og_main_menu_add" />
    <fragment
        android:id="@+id/reviewsFragment"
        android:name="com.tenutz.storemngsim.ui.review.ReviewsFragment"
        android:label="ReviewsFragment"
        tools:layout="@layout/fragment_reviews">
        <action
            android:id="@+id/action_reviewsFragment_to_storeReplyPostFragment"
            app:destination="@id/storeReplyPostFragment" />
        <action
            android:id="@+id/action_reviewsFragment_to_menuReplyPostFragment"
            app:destination="@id/menuReplyPostFragment" />
    </fragment>
    <fragment
        android:id="@+id/storeReplyPostFragment"
        android:name="com.tenutz.storemngsim.ui.review.store.StoreReplyPostFragment"
        android:label="StoreReplyPostFragment"
        tools:layout="@layout/fragment_store_reply_post" />
    <fragment
        android:id="@+id/menuReplyPostFragment"
        android:name="com.tenutz.storemngsim.ui.review.menu.MenuReplyPostFragment"
        android:label="MenuReplyPostFragment"
        tools:layout="@layout/fragment_menu_reply_post" />
    <fragment
        android:id="@+id/helpsFragment"
        android:name="com.tenutz.storemngsim.ui.help.HelpsFragment"
        android:label="HelpsFragment"
        tools:layout="@layout/fragment_helps" />
    <fragment
        android:id="@+id/statisticsFragment"
        android:name="com.tenutz.storemngsim.ui.statistics.StatisticsFragment"
        android:label="StatisticsFragment"
        tools:layout="@layout/fragment_statistics" />
    <include app:graph="@navigation/navigation_main_category" />
    <include app:graph="@navigation/navigation_middle_category" />
    <include app:graph="@navigation/navigation_sub_category" />
    <include app:graph="@navigation/navigation_main_menu" />
</navigation>