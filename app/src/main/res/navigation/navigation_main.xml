        android:name="com.tenutz.storemngsim.ui.menu.mainmenu.optiongroup.MmOptionGroupsFragment"
        android:label="MmOptionGroupsFragment"
        tools:layout="@layout/fragment_mm_option_groups">
        <action
            android:id="@+id/action_mmOptionGroupsFragment_to_mmOptionGroupsEditFragment"
            app:destination="@id/mmOptionGroupsEditFragment" />
    </fragment>
    <fragment
        android:id="@+id/mmOptionGroupsEditFragment"
        android:name="com.tenutz.storemngsim.ui.menu.mainmenu.optiongroup.MmOptionGroupsEditFragment"
        android:label="MmOptionGroupsEditFragment"
        tools:layout="@layout/fragment_mm_option_groups_edit" />
    <fragment
        android:id="@+id/optionMenusFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optionmenu.OptionMenusFragment"
        android:label="OptionMenusFragment"
        tools:layout="@layout/fragment_option_menus">
        <action
            android:id="@+id/action_optionMenusFragment_to_optionMenusEditFragment"
            app:destination="@id/optionMenusEditFragment" />
        <action
            android:id="@+id/action_optionMenusFragment_to_optionMenuAddFragment"
            app:destination="@id/optionMenuAddFragment" />
        <action
            android:id="@+id/action_optionMenusFragment_to_optionMenuDetailsFragment"
            app:destination="@id/optionMenuDetailsFragment" />
        <action
            android:id="@+id/action_optionMenusFragment_to_omOptionGroupsFragment"
            app:destination="@id/omOptionGroupsFragment" />
    </fragment>
    <fragment
        android:id="@+id/optionMenusEditFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optionmenu.OptionMenusEditFragment"
        android:label="OptionMenusEditFragment"
        tools:layout="@layout/fragment_option_menus_edit" />
    <fragment
        android:id="@+id/optionMenuAddFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optionmenu.OptionMenuAddFragment"
        android:label="OptionMenuAddFragment"
        tools:layout="@layout/fragment_option_menu_add" />
    <fragment
        android:id="@+id/optionMenuDetailsFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optionmenu.OptionMenuDetailsFragment"
        android:label="OptionMenuDetailsFragment"
        tools:layout="@layout/fragment_option_menu_details" />
    <fragment
        android:id="@+id/omOptionGroupsFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optionmenu.optiongroup.OmOptionGroupsFragment"
        android:label="OmOptionGroupsFragment"
        tools:layout="@layout/fragment_om_option_groups">
        <action
            android:id="@+id/action_omOptionGroupsFragment_to_omOptionGroupsEditFragment"
            app:destination="@id/omOptionGroupsEditFragment" />
    </fragment>
    <fragment
        android:id="@+id/omOptionGroupsEditFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optionmenu.optiongroup.OmOptionGroupsEditFragment"
        android:label="OmOptionGroupsEditFragment"
        tools:layout="@layout/fragment_om_option_groups_edit" />
    <fragment
        android:id="@+id/optionGroupsFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optiongroup.OptionGroupsFragment"
        android:label="OptionGroupsFragment"
        tools:layout="@layout/fragment_option_groups">
        <action
            android:id="@+id/action_optionGroupsFragment_to_optionGroupsEditFragment"
            app:destination="@id/optionGroupsEditFragment" />
        <action
            android:id="@+id/action_optionGroupsFragment_to_optionGroupAddFragment"
            app:destination="@id/optionGroupAddFragment" />
        <action
            android:id="@+id/action_optionGroupsFragment_to_optionGroupDetailsFragment"
            app:destination="@id/optionGroupDetailsFragment" />
        <action
            android:id="@+id/action_optionGroupsFragment_to_ogMappingMenusFragment"
            app:destination="@id/ogMappingMenusFragment" />
    </fragment>
    <fragment
        android:id="@+id/optionGroupsEditFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optiongroup.OptionGroupsEditFragment"
        android:label="OptionGroupsEditFragment"
        tools:layout="@layout/fragment_option_groups_edit" />
    <fragment
        android:id="@+id/optionGroupAddFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optiongroup.OptionGroupAddFragment"
        android:label="OptionGroupAddFragment"
