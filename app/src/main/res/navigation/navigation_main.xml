        android:id="@+id/ogMainMenuAddFragment"
        android:name="com.tenutz.storemngsim.ui.menu.optiongroup.mainmenu.OgMainMenuAddFragment"
        android:label="OgMainMenuAddFragment"
        tools:layout="@layout/fragment_og_main_menu_add" />
    <fragment
        android:id="@+id/reviewsFragment"
        android:name="com.tenutz.storemngsim.ui.review.ReviewsFragment"
        android:label="ReviewsFragment"
        tools:layout="@layout/fragment_reviews">
        <action
            android:id="@+id/action_reviewsFragment_to_storeReplyPostFragment"
            app:destination="@id/storeReplyPostFragment" />
        <action
            android:id="@+id/action_reviewsFragment_to_menuReplyPostFragment"
            app:destination="@id/menuReplyPostFragment" />
    </fragment>
    <fragment
        android:id="@+id/storeReplyPostFragment"
        android:name="com.tenutz.storemngsim.ui.review.store.StoreReplyPostFragment"
        android:label="StoreReplyPostFragment"
        tools:layout="@layout/fragment_store_reply_post" />
    <fragment
        android:id="@+id/menuReplyPostFragment"
        android:name="com.tenutz.storemngsim.ui.review.menu.MenuReplyPostFragment"
        android:label="MenuReplyPostFragment"
        tools:layout="@layout/fragment_menu_reply_post" />
    <fragment
        android:id="@+id/helpsFragment"
        android:name="com.tenutz.storemngsim.ui.help.HelpsFragment"
        android:label="HelpsFragment"
        tools:layout="@layout/fragment_helps" />
    <fragment
        android:id="@+id/statisticsFragment"
        android:name="com.tenutz.storemngsim.ui.statistics.StatisticsFragment"
        android:label="StatisticsFragment"
        tools:layout="@layout/fragment_statistics" />
    <navigation android:id="@+id/navigation_main_category"
        app:startDestination="@id/mainCategoriesFragment"
        tools:ignore="InvalidNavigation">
        <action
            android:id="@+id/action_mainCategoriesFragment_to_middleCategoriesFragment"
            app:destination="@id/middleCategoriesFragment" />
        <fragment
            android:id="@+id/mainCategoryAddFragment"
            android:name="com.tenutz.storemngsim.ui.menu.category.main.MainCategoryAddFragment"
            tools:layout="@layout/fragment_main_category_add"
            android:label="MainCategoryAddFragment" />
        <fragment
            android:id="@+id/mainCategoryDetailsFragment"
            android:name="com.tenutz.storemngsim.ui.menu.category.main.MainCategoryDetailsFragment"
            tools:layout="@layout/fragment_main_category_details"
            android:label="MainCategoryDetailsFragment">
            <argument
                android:name="mainCategoryCode"
                app:argType="string" />
        </fragment>
        <fragment
            android:id="@+id/mainCategoriesEditFragment"
            android:name="com.tenutz.storemngsim.ui.menu.category.main.MainCategoriesEditFragment"
            tools:layout="@layout/fragment_main_categories_edit"
            android:label="MainCategoriesEditFragment">
            <argument
                android:name="mainCategories"
                app:argType="com.tenutz.storemngsim.data.datasource.api.dto.category.MainCategoriesResponse" />
        </fragment>
        <fragment
            android:id="@+id/mainCategoriesFragment"
            android:name="com.tenutz.storemngsim.ui.menu.category.main.MainCategoriesFragment"
            tools:layout="@layout/fragment_main_categories"
            android:label="MainCategoriesFragment">
            <action
                android:id="@+id/action_mainCategoriesFragment_to_mainCategoriesEditFragment"
                app:destination="@id/mainCategoriesEditFragment" />
            <action
                android:id="@+id/action_mainCategoriesFragment_to_mainCategoryAddFragment"
                app:destination="@id/mainCategoryAddFragment" />
            <action
                android:id="@+id/action_mainCategoriesFragment_to_mainCategoryDetailsFragment"
                app:destination="@id/mainCategoryDetailsFragment" />
        </fragment>
    </navigation>
</navigation>